{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["es5", "es2015", "es2016", "es2017", "dom"]
  },
  "exclude": [
    "node_modules"
  ]
}
